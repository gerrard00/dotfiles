{
    "vim.disableAnnoyingNeovimMessage": true,
    "files.exclude": {
        "**/*.swp": true
    },
    "editor.fontSize": 12,
    "workbench.colorTheme": "Nord",
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}